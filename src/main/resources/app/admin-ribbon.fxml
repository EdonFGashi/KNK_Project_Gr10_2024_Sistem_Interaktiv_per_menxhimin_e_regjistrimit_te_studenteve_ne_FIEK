<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="55.0" prefWidth="600.0" spacing="30.0" style="-fx-background-color: #4CC2FF; -fx-background-radius: 0.5;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminRibbonController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Label onMouseClicked="#handleProfileClick" prefHeight="18.0" prefWidth="51.0" text="PROFILE" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label layoutX="20.0" layoutY="23.0" onMouseClicked="#handleInboxClick" prefHeight="18.0" prefWidth="35.0" text="INBOX" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label onMouseClicked="#handleStudentMenagmentClick" prefHeight="30.0" prefWidth="66.0" text="STUDENT &#10;MENAGMENT" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Arial" size="10.0" />
         </font>
      </Label>
      <Label layoutX="147.0" layoutY="20.0" onMouseClicked="#handleSupervisorMenagmentClick" prefHeight="30.0" prefWidth="66.0" text="SUPERVISOR&#10;MENAGMENT" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Arial" size="10.0" />
         </font>
      </Label>
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</HBox>
